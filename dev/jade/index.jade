                            i.fa.fa-arrow-right(aria-hidden='true')
                    li.color555(style="margin-bottom: -1px;")
                        a(href='#napolnenie-tab') Наполнение
                            i.fa.fa-arrow-right(aria-hidden='true')
                    li.color555#top-furnitura-tab(style="margin-bottom: -1px;")
                        a(href='#furnitura-tab') Фуринтура
                .tab-content
                    include parts/profil-tab
                    include parts/napolnenie-tab
                    include parts/furnitura-tab
        include parts/price-block
        include parts/hide.html
        //include parts/load-img.jade
        include parts/napolnenie-block.jade
        include parts/pokraska-block.jade
        include parts/hide.jade
        script.
            function minimalPriceContent() {
                SelectTabKarkasEnd('Tur', 'K');
                SelectKreplenieEnd('img/addontoprofil/Euroshop/kreplenie-otkritoe.png', 'Открытое', 'E401/E401', '35');
                SelectUplotnitelEnd('img/addontoprofil/Euroshop/uplotnitel-2.png', 'белый', 'K402', '18.05');
                SelectZaglushkaEnd('img/addontoprofil/Euroshop/zaglushka-dek-v-paz-alternativa.png', 'белый', 'K404', '20.54');
                SelectZaglushkaTorcevayaEnd('img/addontoprofil/Euroshop/zaglushka-dek.png', 'белый', 'A14', '0');
                var state1 = window.parent.$('iframe#state1');
                var state2 = window.parent.$('iframe#state2');
                var minimalMaterialArray = [['stekloObyichnoe', 1],
                                            ['LDSP', 2],
                                            ['DSPshlifovannyiy', 3],
                                            ['MDFshlifovannyiy', 4],
                                            ['PVHvspenennyiy', 5],
                                            ['polikarbonatSotovyiy', 6],
                                            ['fanera', 7],
                                            ['gipsovinil12', 8],
                                            ['gipsodekor12', 9],
                                            ['gipsokarton12', 10]];
                //-for (var j = 1; j <= state1.contents().find('#TOTAL_PAINTING_ID').val(); j++) {
                    //-for (var i = 1; i <= 5; i++) {
                        //-if (state1.contents().find('*[data-id=' + j + ']').find('.material-' + i + '-type').text() != '') {
                            //-state2.contents().find('*[data-id=' + j + ']').find('.material-' + i + '-type').text(minimalMaterialArray[i-1][0]);
                        //-}
                    //-}
                //-}
                for (var i = 1; i <= 5; i++) {
                    if (state1.contents().find('*[data-id=' + 1 + ']').find('.material-' + i + '-type').text() != '') {
                        addMaterialBtn(); SelectMaterialEnd(minimalMaterialArray[i - 1][0], minimalMaterialArray[i - 1][1]);
                    }
                }
            }
            function iframeRedraw(e) {
                if (window.parent.$('iframe#state1').get(0).style.display != 'none') var fr = window.parent.$('iframe#state1');
                if (window.parent.$('iframe#state2').get(0).style.display != 'none') var fr = window.parent.$('iframe#state2');
                if (window.parent.$('iframe#state3').get(0).style.display != 'none') var fr = window.parent.$('iframe#state3');
                var el = fr.contents().find(e);
                if (el.attr('aria-expanded') === 'true') fr.height(fr.contents().height() - el.height());
            }
        script(src='js/script.js')
